quired>\r\n            <label for=\"email\"\r\n              class=\"absolute -top-1.5 left-2 text-gray-600 text-xs peer-placeholder-shown:text-base peer-placeholder-shown:text-gray-400 peer-placeholder-shown:top-4 transition-all peer-focus:-top-1.5 peer-focus:text-gray-600 peer-focus:text-xs w-4/5 md:w-auto cursor-text select-none\">Email:</label>\r\n          </div>\r\n          <div class=\"w-full md:w-2/5 relative\">\r\n            <input type=\"text\" name=\"profession\" id=\"profession\"\r\n              class=\"placeholder-transparent peer mt-4 border-b-2 focus:outline-none focus:border-lic-blue focus:border-opacity-100 px-2 pb-1 w-4/5 md:w-11/12 lg:w-auto\"\r\n              placeholder=\"Profession\" maxlength=\"50\" required>\r\n            <label for=\"profession\"\r\n              class=\"absolute -top-1.5 left-2 text-gray-600 text-xs peer-placeholder-shown:text-base peer-placeholder-shown:text-gray-400 peer-placeholder-shown:top-4 transition-all peer-focus:-top-1.5 peer-focus:text-gray-600 peer-focus:text-xs w-4/5 md:w-auto cursor-text select-none\">Profession:</label>\r\n          </div>\r\n          <div class=\"w-full md:w-2/5 relative\">\r\n            <input type=\"text\" name=\"city\" id=\"city\"\r\n              class=\"placeholder-transparent peer mt-4 border-b-2 focus:outline-none focus:border-lic-blue focus:border-opacity-100 px-2 pb-1 w-4/5 md:w-11/12 lg:w-auto\"\r\n              placeholder=\"City\" maxlength=\"50\" required>\r\n            <label for=\"city\"\r\n              class=\"absolute -top-1.5 left-2 text-gray-600 text-xs peer-placeholder-shown:text-base peer-placeholder-shown:text-gray-400 peer-placeholder-shown:top-4 transition-all peer-focus:-top-1.5 peer-focus:text-gray-600 peer-focus:text-xs w-4/5 md:w-auto cursor-text select-none\">City:</label>\r\n          </div>\r\n          <div class=\"w-full md:w-2/5 relative\">\r\n            <input type=\"text\" name=\"address\" id=\"address\"\r\n              class=\"placeholder-transp